    <declare-styleable name="FlikerProgressBar">
        <attr format="dimension" name="textSize"/>
        <attr format="color" name="loadingColor"/>
        <attr format="color" name="stopColor"/>
        <attr format="color" name="finishColor"/>
        <attr format="dimension" name="radius"/>
        <attr format="dimension" name="borderWidth"/>
    </declare-styleable>
    <declare-styleable name="GroupRecyclerView">
        <attr format="dimension" name="group_height"/>
        <attr format="dimension" name="group_child_offset"/>
        <attr format="color" name="group_background"/>
        <attr format="color" name="group_text_color"/>
        <attr format="dimension" name="group_text_size"/>
        <attr format="boolean" name="group_center"/>
